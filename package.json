{
  "name": "movie-api",
  "private": true,
  "version": "1.0.0",
  "description": "Movie app",
  "main": "config.js",
  "scripts": {
    "test": "xo",
    "lint": "xo",
    "format": "xo --fix",
    "start": "pm2 start --interpreter none 'node ./dist/bin/start.js'",
    "build": "./node_modules/.bin/tsc",
    "postbuild": "npm run migrate up",
    "compile": "./node_modules/.bin/tsc-watch --onSuccess \"node ./dist/bin/start.js \"",
    "dev": "nodemon --exec \"npm run compile\"",
    "migrate": "node ./bin/migrate.js",
    "migrate:create": "migrate create --migrations-dir='./src/migrations'"
  },
  "keywords": [
    "express",
    "postgres"
  ],
  "author": "Aniefiok Akpan",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.0",
    "client-sessions": "^0.8.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "migrate": "^1.7.0",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pm2": "^5.1.2",
    "sql-template-strings": "^2.2.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.11",
    "nodemon": "^2.0.7",
    "tsc-watch": "^4.2.9",
    "xo": "^0.36.1"
  },
  "xo": {
    "prettier": true,
    "space": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanio/movie-api.git"
  }
}
